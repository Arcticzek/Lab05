#include <iostream>

using namespace std;

void zamien(int& a, int& b) {
    cout << "Wartosc A: " << a << endl;
    cout << "Wartosc B: " << b << endl;
    int temp = a;
    a = b;
    b = temp;
    cout << "Wartosc A: " << a << endl;
    cout << "Wartosc B: " << b;
}

int main()
{
    int x, y;

    cout << "Podaj dwie liczby, aby zamienić wartości między nimi: ";
    cin >> x;
    cin >> y;
    zamien(x, y);
    return 0;
}
